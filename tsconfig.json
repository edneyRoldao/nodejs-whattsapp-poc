{
  "compilerOptions": {
    "target": "es5",
    "module": "CommonJS",
    "lib": ["es6"],
    "allowJs": true,
    "outDir": "Build",
    "rootDir": "src",
    "strict": true,
    "strictPropertyInitialization": false,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
